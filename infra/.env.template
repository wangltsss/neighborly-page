# AWS Account Configuration
# Copy this file to .env and fill in your values

# Your AWS Account ID (find it in AWS Console top right corner)
AWS_ACCOUNT_ID=123456789012

# AWS Region to deploy to (recommended: us-east-1 for lowest latency)
AWS_REGION=us-east-1

# Optional: Resource name prefix (defaults to 'neighborly')
# You can add your name for clarity, e.g., 'jiahao-neighborly'
RESOURCE_PREFIX=neighborly

# Optional: Your developer name/identifier (for tagging and identification)
DEVELOPER_NAME=your-name
