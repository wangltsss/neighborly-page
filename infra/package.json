    "seed:clear": "ts-node seed-data.ts --clear"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "aws-cdk": "2.116.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.700.0",
    "@aws-sdk/client-dynamodb": "^3.700.0",
    "@aws-sdk/util-dynamodb": "^3.700.0",
    "aws-cdk-lib": "2.116.0",
    "constructs": "10.3.0",
    "dotenv": "^16.3.1"
  }
}
