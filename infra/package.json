{
  "name": "neighborly-infra",
  "version": "0.1.0",
  "description": "AWS CDK infrastructure for Neighborly community platform",
  "private": true,
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "cdk": "cdk",
